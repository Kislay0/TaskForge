{
  "name": "taskforge",
  "lockfileVersion": 3,
  "requires": true,
  "packages": {
    "": {
      "name": "taskforge",
      "workspaces": [
        "apps/*",
        "packages/*"
      ],
      "engines": {
        "node": ">=22 <23"
      }
    },
    "apps/api": {
      "name": "@taskforge/api"
    },
    "apps/web": {
      "name": "@taskforge/web"
    },
    "apps/worker": {
      "name": "@taskforge/worker"
    },
    "node_modules/@taskforge/api": {
      "resolved": "apps/api",
      "link": true
    },
    "node_modules/@taskforge/db": {
      "resolved": "packages/db",
      "link": true
    },
    "node_modules/@taskforge/types": {
      "resolved": "packages/types",
      "link": true
    },
    "node_modules/@taskforge/utils": {
      "resolved": "packages/utils",
      "link": true
    },
    "node_modules/@taskforge/web": {
      "resolved": "apps/web",
      "link": true
    },
    "node_modules/@taskforge/worker": {
      "resolved": "apps/worker",
      "link": true
    },
    "packages/db": {
      "name": "@taskforge/db"
    },
    "packages/types": {
      "name": "@taskforge/types"
    },
    "packages/utils": {
      "name": "@taskforge/utils"
    }
  }
}
